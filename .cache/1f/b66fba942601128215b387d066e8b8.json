","generated":{"line":44,"column":51},"source":"src/feature-pagination.js","original":{"line":65,"column":54}},{"generated":{"line":44,"column":61},"source":"src/feature-pagination.js","original":{"line":65,"column":27}},{"generated":{"line":44,"column":62},"source":"src/feature-pagination.js","original":{"line":65,"column":18}},{"generated":{"line":44,"column":63},"source":"src/feature-pagination.js","original":{"line":65,"column":8}},{"generated":{"line":45,"column":0},"source":"src/feature-pagination.js","original":{"line":66,"column":5}},{"generated":{"line":46,"column":0},"source":"src/feature-pagination.js","original":{"line":67,"column":1}},{"generated":{"line":46,"column":1},"source":"src/feature-pagination.js","original":{"line":59,"column":0}}],"sources":{"src/feature-pagination.js":"//Arary with the elements\r\nlet arr =[\r\n    \r\n        `<span>hello1</span>`,\r\n        `<span>hello2</span>`,\r\n        `<span>hello3</span>`,\r\n        `<span>hello4</span>`,\r\n        `<span>hello5</span>`,\r\n        `<span>hello6</span>`,\r\n        `<span>hello7</span>`,\r\n        `<span>hello8</span>`,\r\n    \r\n]\r\n\r\n//State which will be updated\r\nconst state = {\r\n    'pageSize': 3,\r\n    'pageNumber': 1\r\n}\r\n\r\n//generate slice of the array that we wanna display\r\nconst paginate = (obj, pageSize, pageNumber)=>{   \r\n    \r\n    let current = (pageNumber-1)*pageSize;\r\n    let endData = current + pageSize;\r\n    \r\n    return obj.slice(current, endData);\r\n}\r\n\r\n//generate buttons\r\nconst generatePage = ()=>{\r\n    const buttons = document.querySelector('#pages');\r\n        if(state.pageNumber === 1){\r\n        buttons.innerHTML = ` <button id='btn2'>Page ${state.pageNumber+1}</button>`;\r\n        }\r\n        \r\n        else if(state.pageNumber>1 && state.pageNumber < Math.ceil(arr.length/state.pageSize)){      \r\n        buttons.innerHTML = `\r\n            <button id='btn1'>Page ${state.pageNumber-1}</button> <br>\r\n            <button id='btn2'>Page ${state.pageNumber+1}</button>\r\n        `;\r\n        }else{\r\n            buttons.innerHTML = `<button id='btn1'>Page ${state.pageNumber-1}</button>`;\r\n        }\r\n}\r\n\r\n//appending elements the the DOM\r\nconst appendData = (result)=>{\r\n    generatePage();\r\n    let divData = document.querySelector('#data');\r\n    divData.innerHTML = result;\r\n}\r\n\r\n//default data when reloading the site\r\nappendData(paginate(arr,state.pageSize, state.pageNumber));\r\n\r\n\r\n//switch pages\r\ndocument.addEventListener('click', (e)=>{\r\n    if(e.target && e.target.id== 'btn2'){\r\n        state.pageNumber += 1;\r\n        appendData(paginate(arr,state.pageSize, state.pageNumber));\r\n    }else if(e.target && e.target.id == 'btn1'){\r\n        state.pageNumber -=1;\r\n        appendData(paginate(arr,state.pageSize, state.pageNumber));\r\n    }\r\n})\r\n\r\n"},"lineCount":null}},"error":null,"hash":"88545f874d423dc42d0ca20d0832a433","cacheData":{"env":{}}}