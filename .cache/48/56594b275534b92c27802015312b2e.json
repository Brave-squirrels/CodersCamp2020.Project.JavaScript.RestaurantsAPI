"style.css":"/*Setup*/\r\n:root{\r\n  --bodyBg: rgba(0,0,0,0.2);\r\n  --mainBackground:rgba(0,0,0,0.3);\r\n  --secondBackground: rgba(0,0,0,0.1);\r\n  --favList: #7D7D7D;\r\n  --mainFont: 'Roboto', sans-serif;\r\n  --logoFont: 'Montserrat', sans-serif;\r\n  --buttonBg: rgba(0,0,0,0.2);\r\n  --inputColor: #E8E8E8;\r\n  --titleColor: #F7F7F7;\r\n }\r\n@import url('https://fonts.googleapis.com/css2?family=Roboto&display=swap');\r\n@import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@600&display=swap');\r\n*{\r\n  box-sizing: border-box;\r\n  letter-spacing: 0.04em;\r\n}\r\nbody{\r\n  padding: 0;\r\n  margin: 0;\r\n  height: 100vh;\r\n  background: var(--bodyBg);\r\n  font-family: var(--mainFont);\r\n}\r\n\r\n/* Scroll styles */\r\nbody::-webkit-scrollbar {\r\n  width: 4px;\r\n}\r\nbody::-webkit-scrollbar-track {\r\n  background: #ddd;\r\n}\r\nbody::-webkit-scrollbar-thumb {\r\n  background: #444;\r\n  border-radius: 4px;\r\n}\r\nhtml{\r\n    scroll-behavior: smooth;\r\n}\r\n\r\n/*padding standards - demo */\r\n\r\nheader, main, nav, #restaurant, footer{\r\n  padding: 0.6em 1em;\r\n}\r\nmain div, #dailtMenu, #reviews, nav div{\r\n  padding: 0.3em 0.8em;\r\n}\r\n\r\n#addReview, #review{\r\n  padding: 0.1em 0.5em;\r\n}\r\n\r\n#mainRight, #mainLeft{\r\n  padding: 2em;\r\n}\r\n\r\n  /* Main styles */\r\n  \r\n  /* Fav restaurants list styles */\r\n\r\n   #favList{\r\n     box-sizing: border-box;\r\n     background: var(--favList);\r\n     position: fixed;\r\n     z-index: 1;\r\n     height: 100%;\r\n     width: 0;\r\n     top: 0;\r\n     right: -4em;\r\n     transition: 0.5s;\r\n     font-size: 1em;\r\n     padding: 1em 2em;\r\n   }\r\n\r\n   #closeFav{\r\n     position: absolute;\r\n     top: 0; \r\n     right: 0.3em;\r\n     font-size: 2em;\r\n     cursor: pointer;\r\n   }\r\n\r\n\r\n  /* header styles */\r\n  header{\r\n    background: var(--mainBackground);\r\n    height: 15%;\r\n    display: grid;\r\n    grid-template-columns: 1fr 15%;\r\n    font-size: 1.2em;\r\n    font-family: var(--logoFont);\r\n  }\r\n  header #siteTitle, header #favTrigger{\r\n    display: flex;\r\n    align-items: center;\r\n  }\r\n  #logo{\r\n    height: 2.2em;\r\n    width: auto;\r\n  }\r\n  #logoTitle{\r\n    padding-left: 0.7em;\r\n    font-weight: bold;\r\n    text-align: center;\r\n  }\r\n  #favIcon{\r\n    height: 1.7em;\r\n    width: auto;\r\n    cursor: pointer;\r\n  }\r\n  #favTrigger{\r\n    justify-content: flex-end;\r\n  }\r\n\r\n\r\n  /* Main search block styles */\r\n  main{\r\n    background: var(--secondBackground);\r\n    height: 78%;\r\n    display: grid;\r\n    grid-template-rows: 40% 60%; \r\n  }\r\n  \r\n  #mainLeft, #mainRight{\r\n    position: relative;\r\n  }\r\n\r\n  #formTitle{\r\n    color: var(--titleColor);\r\n    font-size: 1.2em;\r\n  }\r\n\r\n  main form input{\r\n    margin-top: 0.5em;\r\n  }\r\n\r\n  main img{ \r\n    height: 3.5em;\r\n    width: auto;\r\n    position: absolute;\r\n    right: 0;\r\n    bottom: 0;\r\n  }\r\n\r\n  #mainRight{\r\n    font-size: 1.1em;\r\n    color: var(--titleColor);\r\n  }\r\n\r\n  /* List of restaurants styles Edit when API\r\n    Display: none; by default\r\n    display: grid; with js\r\n  */\r\n  nav{\r\n    background: var(--bodyBg);\r\n    display: grid;\r\n    grid-gap: 0.5em;\r\n  }\r\n  nav div{\r\n    width: 100%;\r\n    height: 100%;\r\n  }\r\n\r\n  #paginationContainer, #filterRestaurants{\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n\r\n  #resDiv span{\r\n    display: block;\r\n  }\r\n  \r\n  #resDiv img{\r\n    height: 0.9em;\r\n    width: auto;\r\n    position: absolute;\r\n    right: 1em;\r\n    top: 50%;\r\n    transform: translateY(-50%);\r\n  }\r\n\r\n  #resDiv{\r\n    box-shadow: -1px 2px 3px 3px rgba(0,0,0,0.35);\r\n    background:#A6A6A6;\r\n    border-radius: 8px;\r\n    width: 100%;\r\n    display: grid;\r\n    cursor: pointer;\r\n    position: relative;\r\n    color: var(--titleColor);\r\n    max-width: 650px;\r\n    margin: 0 auto;\r\n  }\r\n  #resTitle,#restaurantName{\r\n    font-weight: bold;\r\n    font-size: 1.1em;\r\n  }\r\n  #resAdr, #restaurantAdress{\r\n    font-size: 0.9em;\r\n  }\r\n\r\n\r\n  /* Display restaurant styles Edit when API\r\n    Display: none; by default\r\n    Display: block; with js\r\n  */\r\n  #restaurant{\r\n    background: var(--secondBackground);\r\n    display: grid;\r\n    position: relative;\r\n    color: var(--titleColor);\r\n    grid-gap: 0.6em;\r\n  }\r\n  \r\n  #restaurant img{\r\n    height: 5em;\r\n    width: auto;\r\n  }\r\n\r\n  #addFav{\r\n    position: absolute;\r\n    top: 0.4em;\r\n    right: 0.3em;\r\n  }\r\n  #restaurantAvgRating span, #restaurantAvgCost span{\r\n    font-weight: bold;\r\n  }\r\n\r\n\r\n  /* Footer styles */\r\n  footer{\r\n    background: var(--mainBackground);\r\n    height: 7%;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n  \r\n\r\n  /* button styles */\r\n  input[type='submit'], button{\r\n    border: 2px solid var(--buttonBg);\r\n    border-radius: 8px;\r\n    padding: 0.2em 0.8em;\r\n    font-size: 1em;\r\n    font-family: var(--mainFont);\r\n    font-weight: bold;\r\n    background: var(--mainBackground);\r\n    color: var(--inputColor);\r\n    cursor: pointer;\r\n    outline: none;\r\n  }\r\n\r\n  /*input text style */\r\n  input[type='text']{\r\n    background: var(--mainBackground);\r\n    border: 2px solid var(--buttonBg);\r\n    border-radius: 4px;\r\n    font-size: 0.9em;\r\n    padding: 0.2em 0.4em;\r\n    color: var(--inputColor);\r\n    font-family: var(--mainFont);\r\n    outline: none; \r\n  }\r\n\r\n  /* Media queries */\r\n  @media only screen and (min-width: 700px){\r\n    :root{\r\n      font-size: 18px;\r\n    }\r\n  }\r\n  @media only screen and (min-width: 1080px){\r\n    main{\r\n      grid-template-columns: 40% 60%; \r\n      grid-template-rows: 100%;\r\n    }\r\n    :root{\r\n      font-size: 20px;\r\n    }\r\n  }"},"lineCount